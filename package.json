{
  "name": "svelte-ts-progtable",
  "svelte": "src/main.ts",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/svelte-ts-progtable.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "validate": "svelte-check",
    "test": "jest",
    "svelte-dts": "svelte-dts"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.11.5",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/svelte": "^3.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "@types/jest": "^26.0.14",
    "babel-jest": "^26.5.2",
    "jest": "^26.5.2",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-dts": "^0.3.2",
    "svelte-jester": "^1.1.5",
    "svelte-preprocess": "^4.0.0",
    "ts-jest": "^26.4.1",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "keywords": [
    "svelte table datatable datagrid progtable prog-table typescript"
  ],
  "files": [
    "src",
    "dist"
  ],
  "description": "A simple datatable component for Svelte using typescript",
  "version": "0.0.1",
  "jest": {
    "transform": {
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  },
  "directories": {
    "example": "example"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prognoshealth/svelte-ts-progtable.git"
  },
  "author": "Aidan Reynolds, Principal Engineer, Prognos Health",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prognoshealth/svelte-ts-progtable/issues"
  },
  "homepage": "https://prognoshealth.com/"
}
